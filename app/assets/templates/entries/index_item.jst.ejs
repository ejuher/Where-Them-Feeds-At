<div class="col-lg-12">
	<div class="panel panel-default entry-index-item">
	  <div class="panel-heading">
	  	<% t = new Date(entry.escape('published')) %>
	    <h3 class="panel-title"><%= entry.escape('title') %> <small class="published"><%= t.toLocaleString() %></small></h3>
	  </div>
	  <div class="panel-body">
	  	
	  	
	  	<img src="<%= entry.escape('image') %>">
	    <p><%= entry.escape('summary') %></p>
	  </div>
	</div>			    
</div>
